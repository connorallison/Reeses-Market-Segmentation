proc format;
value my5scale
    1 = "disagree a lot"
    2 = "disagree a little"
    3 = "neither agree nor disagree"
    4 = "agree a little"
    5 = "agree a lot";
value yesno
    0 = "no"
    1 = "yes";
value my3scale
	1 = "never"
	2 = "sometimes"
	3 = "always"
run;

